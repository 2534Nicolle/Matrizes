package lpa2;

public class jeferson {

	public static void main(String[] args) {
    int[][] m = new int[4][4];

	for (int i = 0; i < 4; i++) {
	for (int j = 0; j < 4; j++)	{
		 if (i==j) {
			 m[i][j] = (int)Math.round(Math.random()*100);
		 } 
	   }
    }

	for (int i = 0; i < 4; i++) {
	for (int j = 0; j < 4; j++) {
    System.out.print(m[i][j] + "\t");
    }
	System.out.println();
	}
  }
}
